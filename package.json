{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "remix setup node",
    "start": "node -r dotenv/config ./node_modules/@remix-run/serve/cli.js build"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@remix-run/react": "^1.2.2",
    "@remix-run/serve": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.6",
    "prisma": "^3.10.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
